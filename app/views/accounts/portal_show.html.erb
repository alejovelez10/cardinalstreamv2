<% if @event.state == 1 || @event.state == 4 %>

    <% if viewer_signed_in? || !@event.has_register%>

    <%if @event.state == 4%>
        <%if @event.event_type%>
          
          <%if @navs == "SI" %>
               
                  <%= render "accounts/ondemands/ondemand_audio_movil", event: @event, array: @array, count: @count, sync: @sync%>
               
               <%else%>
                          <%= render "accounts/ondemands/ondemand_audio", event: @event, array: @array, count: @count, sync: @sync%>
               <%end%>



          
        

        <%else%>

            <%if @event.has_ppts && @event.slides != nil%>
              
              <%if @navs == "SI" %>
               
                 <%= render "accounts/ondemands/ondemand_video_ppts_movil", event: @event, array: @array,  count: @count, sync: @sync%>
               
               <%else%>
                         <%= render "accounts/ondemands/ondemand_video_ppts", event: @event, array: @array,  count: @count, sync: @sync%>
               <%end%>

            <%else%>
           
                   <%if @navs == "SI" %>
                          <%= render "accounts/ondemands/ondemand_video_movil", event: @event, array: @array,  count: @count, sync: @sync%>
                    <%else%>
                           <%= render "accounts/ondemands/ondemand_video", event: @event, array: @array,  count: @count, sync: @sync%>
                    <%end%>
            
          


            <%end%>
       


        <%end%>
    
    <%else%>
    
         <%if @event.event_type%>

         <%if @navs == "SI" %>
                          <%= render "accounts/lives/live_audio_movil", event: @event, array: @array, nav: @navs%>
                    <%else%>
                           <%= render "accounts/lives/live_audio", event: @event, array: @array, nav: @navsv%>
                    <%end%>


          

        <%else%>
           <%if !@event.has_ppts%>
                            <%if @navs == "SI" %>
                                  <%= render "accounts/lives/live_video_movil", event: @event, array: @array%>
                            <%else%>
                                  <%= render "accounts/lives/live_video", event: @event, array: @array%>
                            <%end%>


                    <%else%>
                            <%if @navs == "SI" %>
                                 <%= render "accounts/lives/live_video_ppt_movil", event: @event, array: @array%>
                            <%else%>
                                 <%= render "accounts/lives/live_video_ppt", event: @event, array: @array%>
                            <%end%>
                    <%end%>
        <%end%>
    <%end%>

    <%else%>
        <%= render "login" , event: @event%>
    <% end %>

   <%else%>
        <%= render "expectation" , event: @event%>
    <% end %>
